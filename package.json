{
  "name": "js-runner",
  "version": "1.0.0-beta1",
  "description": "Run JavaScript files in NodeJS VM.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "build": "tsc --project tsconfig.build.json && webpack --config ./webpack.config.js",
    "build:dev": "tsc --project tsconfig.build.json && webpack --config ./webpack.config.js --mode=development",
    "start:dev": "ts-node src/index.ts",
    "start": "npm run build && electron ./dist/index.js"
  },
  "author": "Jeppe Lind",
  "license": "MIT",
  "dependencies": {
    "ini": "^3.0.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.7.1",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "react": "^18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "recoil": "^0.7.3-alpha.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
